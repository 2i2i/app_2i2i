{
  "steps": [
   {
      "name": "gcr.io/i2i-test/cloudbuild",
      "entrypoint": "bash",
      "args": [
        "-c",
        "flutter build web && firebase deploy --only hosting --token $$FIREBASE_TOKEN"
      ],
      "secretENV": ["FIREBASE_TOKEN"]
  }],
  "availableSecrets": {
    "secretManager": [{
      "versionName": "projects/i2i-test/secrets/FIREBASE_TOKEN/versions/1",
      "env": "FIREBASE_TOKEN"
  }]
  }
}